    </span><span class="nl">"deposit_address"</span><span class="p">:</span><span class="w"> </span><span class="s2">"0x72012d8af69263a509af8cd522374fc65c454539"</span><span class="p">,</span><span class="w">
    </span><span class="nl">"secondary_address"</span><span class="p">:</span><span class="w"> </span><span class="kc">null</span><span class="w">
</span><span class="p">}</span><span class="w">
