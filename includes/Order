    </span><span class="p">{</span><span class="w">
      </span><span class="nl">"market"</span><span class="p">:</span><span class="w"> </span><span class="s2">"KRW-BTC"</span><span class="p">,</span><span class="w">
      </span><span class="nl">"uuid"</span><span class="p">:</span><span class="w"> </span><span class="s2">"9e8f8eba-7050-4837-8969-cfc272cbe083"</span><span class="p">,</span><span class="w">
      </span><span class="nl">"price"</span><span class="p">:</span><span class="w"> </span><span class="s2">"4280000.0"</span><span class="p">,</span><span class="w">
